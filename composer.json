{
    "name": "jolicode/forecast-php-api",
    "description": "An up to date PHP client for Forecast's API",
    "keywords": ["forecast", "forecast api", "forecastapp", "client", "openapi", "swagger", "sdk"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Xavier Lacot",
            "email": "xlacot@jolicode.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "JoliCode\\Forecast\\Api\\": "generated/",
          "JoliCode\\Forecast\\": "src/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "jane-php/open-api-runtime": "^6.3",
        "php-http/client-implementation": "*",
        "php-http/client-common": "^1.7 || ^2.0"
    },
    "require-dev": {
        "jane-php/open-api-3": "^6.3",
        "symfony/http-client": "^5.2",
        "nyholm/psr7": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.18"
    }
}
